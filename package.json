{
    "name": "ubreakwefix-monorepo",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "cd client && npm install && npm run build && mkdir -p ../public && cp -r dist/* ../public",
        "dev": "concurrently \"node server/index.js\" \"npm run dev --prefix client\""
    },
    "engines": {
        "node": ">=18"
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}